
// gets img progile of user
export default async function getProblems(token,from) {

    const url = import.meta.env.VITE_APP_API_URL;
    const response = await fetch(`${url}api/user/problem/problems?from=${from}`, {
        method : "GET",
        headers: {
            "Authorization": `Bearer ${token}`,
            "Content-Type" :"application/json"
        },
    });
    return response;
}