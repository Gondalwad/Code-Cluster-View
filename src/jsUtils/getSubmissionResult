
export default async function getSubmissionResult(jobId){

    // const url = process.env.VITE_APP_API_URL;
    const token = localStorage.getItem("token");
    const url = import.meta.env.VITE_APP_API_URL;
    console.log("printed : ",jobId);

    const response = await fetch(`${url}api/user/problem/result?jobId=${jobId}`, {
        method : "GET",
        headers: {
            "Authorization": `Bearer ${token}`,
            "Content-Type":"application/json"
        },
    });

    return response;
}